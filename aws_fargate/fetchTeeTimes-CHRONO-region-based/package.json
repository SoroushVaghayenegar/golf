{
  "name": "fetch-tee-times-chrono",
  "version": "1.0.0",
  "description": "Lambda function to fetch tee times from Chrono golf courses",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "deploy": "npm run build && zip -r function.zip .",
